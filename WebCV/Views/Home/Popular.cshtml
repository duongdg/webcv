@model List<WebCV.Models.Top6_Profile_Popular_Result>
<div class="col-md-3 technology-right">
    <div class="blo-top1">
        <div class="tech-btm">
            <div class="search-1 wow fadeInDown" data-wow-duration=".8s" data-wow-delay=".2s">
                <form action="#" method="post">
                    <input type="search" name="Search" value="Search" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Search';}" required="">
                    <input type="submit" value=" ">
                </form>
            </div>
            <h4>Bài đăng phổ biến </h4>
            @foreach (var item in Model)
            {
                <div class="blog-grids wow fadeInDown" data-wow-duration=".8s" data-wow-delay=".2s">
                    <div class="blog-grid-left">
                        <a href="@Url.Action("ResponProfile",new {@Id_Profile=item.Id_Profile})"><img src="~/Content/Home/images/@item.Image" class="img-responsive div-img-home-popular" alt=""></a>
                    </div>
                    <div class="blog-grid-right">

                        <h5><a href="@Url.Action("ResponProfile",new {@Id_Profile=item.Id_Profile})">@item.FullName</a> </h5>
                    </div>
                    <div class="clearfix"> </div>

                </div>
            }
        </div>
    </div>
</div>
<div class="clearfix"></div>
<!-- technology-right -->
