@model List<WebCV.Models.View_Profile_Skill>
<div class="skills-agileinfo" id="skills">
    <div class="container">
        <h3 class="text-center">Kỹ năng</h3>
        <div class="modal-spa">
            <div class="skills">
                <div class="col-md-6 bar-grids bargrids-left">
                    <h4> Các kỹ năng theo tỷ lệ </h4>
                    <p>
                        Sự phân bố tỷ lệ cho từng kỹ năng
                    </p>
                    @foreach (var item in Model)
                    {
                        <h6>@item.SkillName  <span> 80% </span></h6>
                   
                    <div class="progress">
                        <div class="progress-bar progress-bar-striped active" style="width: 80%">
                        </div>
                    </div>
                    }
                </div>
                <div class="col-md-6 bar-grids  w3-agile">
                    <h4> Kỹ năng mềm </h4>
                    <p>
                        Một số kỹ năng mềm của bản thân do tự mình đánh giá.
                    </p>
                    @foreach (var item in Model)
                    {
                        <div class="col-md-4 hobbies-grid1 grid1">
                            <span class="fa fa-codepen"></span>
                            <h5>@item.SkillName</h5>
                        </div>
                    }
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"> </div>
            </div>
        </div>
    </div>
</div>
